<?xml version='1.0' encoding='utf-8'?>
<widget id="com.polymorphiclabs.graph_example" version="0.0.1"
	xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
	<name>Polymorphic Graphing Example</name>
	<description>A sample Polymorphic Dot app that graphs data from
		connected Dots.
	</description>
	<author email="trey@polymorphiclabs.com" href="http://www.polymorphiclabs.com">Polymorphic Labs
	</author>
	<content src="index.html" />
	<plugin name="cordova-plugin-whitelist" spec="1" />
	<access origin="*" />
	<allow-intent href="http://*/*" />
	<allow-intent href="https://*/*" />
	<allow-intent href="tel:*" />
	<allow-intent href="sms:*" />
	<allow-intent href="mailto:*" />
	<allow-intent href="geo:*" />
	<platform name="android">
		<allow-intent href="market:*" />
	</platform>
	<platform name="ios">
		<allow-intent href="itms:*" />
		<allow-intent href="itms-apps:*" />
	</platform>
	<feature name="BLE">
		<param name="android-package" value="com.megster.cordova.ble.central.BLECentralPlugin" />
		<param name="ios-package" value="BLECentralPlugin" />
		<param name="id" value="cordova-plugin-ble-central" />
	</feature>
	<feature name="Compat">
		<param name="id" value="cordova-plugin-compat" />
	</feature>
	<feature name="cordova-plugin-pml-manager">
		<param name="android-package" value="com.polymorphiclabs.cordova.plugin.pml.manager" />
		<param name="ios-package" value="cordova-plugin-pml-manager" />
		<param name="id" value="cordova-plugin-pml-manager" />
	</feature>
	<feature name="Whitelist">
		<param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
		<param name="onload" value="true" />
		<param name="id" value="cordova-plugin-whitelist" />
	</feature>
</widget>
